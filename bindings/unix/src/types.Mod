MODULE types [ INTERFACE "C" ];

IMPORT SYSTEM;
CONST
  _BITS_TYPES_H* = 1;
  _SYS_TYPES_H* = 1;
  __BIT_TYPES_DEFINED__* = 1;
TYPE
  String* = POINTER [CSTRING] TO ARRAY OF CHAR;
  Ptr* = SYSTEM.PTR;
  size_t* = LONGINT;
  __u_char* = CHAR;
  __u_short* = INTEGER;
  __u_int* = LONGINT;
  __u_long* = LONGINT;
  __int8_t* = CHAR;
  __uint8_t* = CHAR;
  __int16_t* = INTEGER;
  __uint16_t* = INTEGER;
  __int32_t* = LONGINT;
  __uint32_t* = LONGINT;
  __quad_t* = RECORD 
    __val* : ARRAY 2 OF LONGINT;
  END;
  __u_quad_t* = RECORD 
    __val* : ARRAY 2 OF __u_long;
  END;
  __dev_t* = __u_quad_t;
  __uid_t* = LONGINT;
  __gid_t* = LONGINT;
  __ino_t* = LONGINT;
  __ino64_t* = __u_quad_t;
  __mode_t* = LONGINT;
  __nlink_t* = LONGINT;
  __off_t* = LONGINT;
  __off64_t* = __quad_t;
  __pid_t* = LONGINT;
  __fsid_t* = RECORD 
    __val* : ARRAY 2 OF LONGINT;
  END;
  __clock_t* = LONGINT;
  __rlim_t* = LONGINT;
  __rlim64_t* = __u_quad_t;
  __id_t* = LONGINT;
  __time_t* = LONGINT;
  __useconds_t* = LONGINT;
  __suseconds_t* = LONGINT;
  __daddr_t* = LONGINT;
  __swblk_t* = LONGINT;
  __key_t* = LONGINT;
  __clockid_t* = LONGINT;
  __timer_t* = SYSTEM.PTR;
  __blksize_t* = LONGINT;
  __blkcnt_t* = LONGINT;
  __blkcnt64_t* = __quad_t;
  __fsblkcnt_t* = LONGINT;
  __fsblkcnt64_t* = __u_quad_t;
  __fsfilcnt_t* = LONGINT;
  __fsfilcnt64_t* = __u_quad_t;
  __ssize_t* = LONGINT;
  __loff_t* = __off64_t;
  __qaddr_t* = POINTER TO __quad_t;
  __caddr_t* = POINTER TO ARRAY OF CHAR;
  __intptr_t* = LONGINT;
  __socklen_t* = LONGINT;
  u_char* = __u_char;
  u_short* = __u_short;
  u_int* = __u_int;
  u_long* = __u_long;
  quad_t* = __quad_t;
  u_quad_t* = __u_quad_t;
  fsid_t* = __fsid_t;
  loff_t* = __loff_t;
  ino_t* = __ino_t;
  dev_t* = __dev_t;
  mode_t* = __mode_t;
  nlink_t* = __nlink_t;
  id_t* = __id_t;
  daddr_t* = __daddr_t;
  caddr_t* = __caddr_t;
  key_t* = __key_t;
  ulong* = LONGINT;
  ushort* = INTEGER;
  uint* = LONGINT;
  int8_t* = CHAR;
  int16_t* = INTEGER;
  int32_t* = LONGINT;
  u_int8_t* = CHAR;
  u_int16_t* = INTEGER;
  u_int32_t* = LONGINT;
  register_t* = LONGINT;
  blksize_t* = __blksize_t;
  blkcnt_t* = __blkcnt_t;
  fsblkcnt_t* = __fsblkcnt_t;
  fsfilcnt_t* = __fsfilcnt_t;
END types.
